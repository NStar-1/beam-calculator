    "svelte": "^4.0.0",
    "svelte-icon": "^1.3.0",
    "svelte-preprocess": "^5.0.0",
    "svelte-sitemap": "^2.6.0",
    "sveltekit-i18n": "^2.4.2",
    "svgo": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^5.0.0",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@carbon/themes": "^11.17.0",
    "@smui/menu": "^7.0.0-beta.8",
    "@smui/switch": "^7.0.0",
    "frame3dd-wasm-js": "github:NStar-1/frame3dd-wasm-js#v0.3.1",
    "inspire-tree": "^7.0.0-beta.2",
